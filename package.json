{
  "name": "apinestjs",
  "version": "0.0.1",
  "description": "open source fun api",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "12.16.3"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rm -rf node_modules/ dist/ package-lock.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.1",
    "@nestjs/core": "^7.0.9",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/platform-socket.io": "^7.0.9",
    "@nestjs/platform-ws": "7.0.9",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/websockets": "7.0.9",
    "@types/dotenv": "^8.2.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "ioredis": "^4.16.3",
    "mongoose": "^5.9.15",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.8.0",
    "rxjs": "^6.5.5",
    "socket.io-redis": "^5.2.0",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "@types/express": "4.17.6",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "26.0.8",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^13.13.5",
    "@types/request-ip": "^0.0.37",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.9",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.5.0",
    "ts-loader": "7.0.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
